{
    "name": "spatie/laravel-server-monitor",
    "description": "Monitor servers",
    "keywords": [
        "spatie",
        "laravel-server-monitor"
    ],
    "homepage": "https://github.com/spatie/laravel-server-monitor",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.0",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "spatie/laravel-blink": "^1.3",
        "spatie/regex": "^3.1",
        "symfony/process": "^6.0|^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.34|^3.7"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\ServerMonitor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\ServerMonitor\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\ServerMonitor\\ServerMonitorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
